<%= form_for edit_task_subtask_path(subtask) do |subtask_form| %>
  <%= subtask_form.fields_for :documents do |f| %>
    <%= f.input :category, collection: Document::CATEGORIES, required: true%>
    <%= f.input :name, required: true %>
    <%= f.input :photo, as: :file, required: true %>
  <% end %>
  <%= subtask_form.submit class: "btn btn-primary" %>
<% end %>
